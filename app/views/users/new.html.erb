<h1>Create New User</h1>
  <%= form_for @user, html: {class: "form-horizontal"}  do |form| %>
    <%= form.hidden_field :organization_id, value: @organization.id %>
    <%= render partial: "user", locals: {form: form} %>
    <%= render partial: "password", locals: {form: form} %>
    <%= submit_tag "Create User", class: "btn btn-primary" %>
  <% end %>
  <br \><br \>
  <%= link_to "Return to Users", users_path %>

