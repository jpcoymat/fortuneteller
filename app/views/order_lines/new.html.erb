<h1>Create New Order Line</h1>
<h3> Enter Order Line Details</h3>
<%= render partial: "order_line_errors" %>
<%= form_for @order_line, html: {class: "form-horizontal"} do |form| %>
  <%= form.hidden_field :organization_id, value: @user.organization_id %>
  <%= form.hidden_field :last_shift_date, value: Date.today %>
  <%= render partial: "order_line", locals: {form: form} %>
  <%= submit_tag "Create Order Line", class: "btn btn-primary" %>
<% end %>

<br \><br \>
<%= link_to "Return to Order Lines", lookup_order_lines_path %>
