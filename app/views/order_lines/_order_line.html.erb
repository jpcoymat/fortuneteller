<div class="form-group">
  <%= form.label "Order Line Reference Number", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.text_field :object_reference_number, class: "form-control"        %>
  </div>
</div>
<div class="form-group">
  <%= form.label "ETD", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.date_select :etd, {start_year: Date.today.year}, class: "form-control" %>
  </div>
</div>
<div class="form-group">
 <%=  form.label "ETA", class: "col-sm-2 control-label" %>
 <div class="col-xs-4">
   <%= form.date_select :eta, {start_year: Date.today.year}, class: "form-control"  %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Product", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.select :product_id, @products.collect {|product| [product.name, product.id]}, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Origin Location", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.select :origin_location_id, @locations.collect {|loc| [loc.name, loc.id]}, {include_blank: true}, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Destination Location", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
  <%= form.select :destination_location_id, @locations.collect {|loc| [loc.name, loc.id]}, {include_blank: true}, class: "form-control" %>
 </div>
</div>
<div class="form-group">
  <%= form.label "Quantity", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
     <%= form.text_field :quantity, class: "form-control"  %>
  </div>
</div>

