
  <%= form.label "Reference Number" %>
  <script>
    $(function() {
      var movement_sources = [
        <% movement_sources.each do |ms| %>
          "<%= ms.object_reference_number %>",
        <% end %>
      ""];
      $( "#<%= source_type%>_object_reference_number" ).autocomplete({
        source: movement_sources
      });
    });
  </script>
  <%= form.text_field :object_reference_number %>
