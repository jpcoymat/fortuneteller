<h1> Create Receipt </h1>
<h4>Enter Receipt Details </h4>
<%= render partial: "receipt_errors" %>
<%= form_for @receipt, html: {class: "form-horizontal"} do |form| %>
  <%= form.hidden_field :organization_id, value: @user.organization_id %>
  <%= render partial: "receipt", locals: {form: form} %>
  <%= submit_tag "Create", class: "btn btn-primary" %>
<% end %>
<br /> <br />
<%= link_to "Return to Receipt", receipts_path %>

