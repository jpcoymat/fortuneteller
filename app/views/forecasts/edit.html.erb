<h1>Edit Forecast</h1>
<h3>Enter Forecast Details</h3>
<%= render partial: "forecast_errors" %>
<%= form_for @forecast, html: {class: "form-horizontal"} do |form| %>
  <%= render partial: "forecast", locals: {form: form} %>
  <%= submit_tag "Update Forecast", {class: "btn btn-primary"}  %>
<% end %>

<br \><br \>
<%= link_to "Return to Forecasts", lookup_forecasts_path %>


