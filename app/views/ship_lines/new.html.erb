<h1>Add Ship Line <h1>
	<h3> Enter Ship Line Details</h3>
	<%= render partial: "ship_line_errors" %>
	<%= form_for @ship_line, html: {class: "form-horizontal"} do |form| %>
          <%= form.hidden_field :last_shift_date, value: Date.today %>
          <%= form.hidden_field :organization_id, value: @user.organization_id %> 
	  <%= render partial: 'ship_line', locals: {form: form} %>
	  <%= submit_tag "Create Ship Line", class: "btn btn-primary" %>
        <% end %>

<br /><br />
<%= link_to "Return to Ship Lines", lookup_ship_lines_path %>


