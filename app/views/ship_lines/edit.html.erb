<h1>Edit Ship Line <h1>
        <h3> Enter Updated Ship Line Details</h3>
        <%= render partial: "ship_line_errors" %>
        <%= form_for @ship_line, html: {class: "form-horizontal"} do |form| %>
          <%= render partial: 'ship_line', locals: {form: form} %>
          <%= submit_tag "Update Ship Line", class: "btn btn-primary" %>
        <% end %>

<br /><br />
<%= link_to "Ship Lines Search Page", lookup_ship_lines_path %> | <%= link_to "Ship Line Details", ship_line_path(@ship_line) %>


